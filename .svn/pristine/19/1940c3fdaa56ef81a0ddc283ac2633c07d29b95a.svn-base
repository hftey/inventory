.report_title
{
	font-size: 16px;
	font-weight: bolder;
}

.report_header
{ 

	font: bold 11px "Trebuchet MS", Verdana, Arial, Helvetica,
	sans-serif;
	color: #6D929B;
	border-left: 1px solid #C1DAD7;
	border-right: 1px solid #C1DAD7;
	border-bottom: 1px solid #C1DAD7;
	border-top: 1px solid #C1DAD7;
	letter-spacing: 2px;
	text-align: left;
	padding: 6px 6px 6px 12px;
	background: #CAE8EA url(/images/table/bg_header.jpg) no-repeat;

}

.link_header
{ 

  
  	font-family: Arial, Helvetica, sans-serif;
	font-size: 10px;
	font-weight: bolder;
	color: #426177;
	text-decoration: none;

}

.link_header:hover
{ font-family: Arial, Verdana, Haettenschweiler; 
  font-size: 10px; 
  color:#6291A7;
}


.report_even.clsSelected {
	background: #ddf2f9;
}

.report_odd.clsSelected {
	background: #ddf2f9;
}


.report_even{ font-family: Arial, Verdana, Haettenschweiler;
	border-right: 1px solid #C1DAD7;
	border-bottom: 1px solid #C1DAD7;
	background: #fff;
	padding: 6px 6px 6px 12px;
	color: #262a2c;
}

TR.report_even > TD {
	border-bottom: solid 1px #CDCDCD;
}

.report_cell{
	border-right: 1px solid #C1DAD7;
	border-left: 1px solid #C1DAD7;
	padding-left: 5px;
	padding-right: 5px;	
	padding-top: 2px;
	padding-bottom: 2px;	
	font-size: 10px;
	
}

.report_odd{ font-family: Arial, Verdana, Haettenschweiler; 
	border-right: 1px solid #C1DAD7;
	border-bottom: 1px solid #C1DAD7;
	background: #F5FAFA;
	padding: 6px 6px 6px 12px;
	color: #262a2c;

}
TR.report_odd > TD {
	border-bottom: solid 1px #CDCDCD;
}

a {
color: # 232300;
}


.report_odd:hover {
	font-family: Arial, Verdana, Haettenschweiler; 

	color:black;
	background-color: #ddf2f9;

}

.report_even:hover {
	font-family: Arial, Verdana, Haettenschweiler; 

	color:black;
	background-color: #ddf2f9;

}

.report_footer {

  background-color: #FFFFFF;
  border-top: solid 1px black;	
}

.report_table_wrapper{
	width: 100%;
}

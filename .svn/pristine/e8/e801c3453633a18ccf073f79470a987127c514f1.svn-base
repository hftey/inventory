@charset "utf-8";
/* Pre-defined Swatch#2 */

/* Containers */
.st-content {
  background: #e1e4e8;
}

.menu-bg {
  background: #798899;
}

.bm-container {
  background: #556270;
}

/* Effect Togglers and Search-box/Login-box Containers */
.side-menu-toggle {
  background: #414b56;
  background-image: url(../img/menu-toggle.png);
  background-repeat: no-repeat;
  background-position: 15px 11px;
}

.side-menu-toggle:focus {
  background: #242b31;
  background-image: url(../img/menu-toggle.png);
  background-repeat: no-repeat;
  background-position: 15px 11px;
}

.login-toggle {
  background: #414b56;
  background-image: url(../img/login.png);
  background-repeat: no-repeat;
  background-position: 10px 11px;
}

.login-toggle:focus {
  background: #242b31;
  background-image: url(../img/login.png);
  background-repeat: no-repeat;
  background-position: 10px 11px;
}

#login-container {
  background: #bac1cb;
  background: rgba(186,193,203,1);
  border: 1px solid #959fae;
  border-top: none;
}

#login-container form button {
  background: #556270;
  color:#FFF;
}

#login-container form button:hover {
  background: #8694a4;
}

.register-forgot a:hover {
  color: #414b56;
}

.search-toggle {
  background: rgb(142,155,170);
  background-image: url(../img/search.png);
  background-position: 9px 9px;
  background-repeat: no-repeat;
  border-left: 1px solid #b1bbc5;
}

#search-container {
  background: #bac1cb;
  background: rgba(186,193,203,1);
  border: 1px solid #959fae;
  color: #414b56;
}

#search-container:focus {
  background: rgb(65,75,86);
  background: rgb(65,75,86,0.7);
  color: #dff1f9;
}

/* Top-nav mega */
.top-nav-mega-1, .top-nav-mega-2, .top-nav-mega-3, .top-nav-mega-4, .top-nav-mega-5, .top-nav-mega-6, .top-nav-mega-full-width {
  background: rgb(56,66,75);
  background: rgba(56,66,75,0.9);
  color: #fff;
}

/* Top-nav mega product nav */
.product-frame {
  background: rgb(56,66,75);
  background: rgba(56,66,75,0.8);
  border: 5px solid #3c454f;
  color: #ddd;
}

.product-frame h1, .product-frame h1 a, .product-frame h2, .product-frame h2 a, .product-frame h3, .product-frame h3 a, .product-frame h4, .product-frame h4 a, .product-frame h5, .product-frame h5 a, .product-frame h6, .product-frame h6 a {
  color: #fff;
  font-weight: 500;
}

.top-nav-mega-full-width, .top-nav-mega-full-width .carousel-caption h4, .top-nav-mega-full-width .carousel-caption h4 a {
  color: #fff;
}

.product-frame:hover {
  background: rgb(60,67,74);
  background: rgba(60,67,74,0.9);
  border: 5px solid #606c77;
}

.top-nav-mega-full-width ul li a, .top-nav-mega-full-width ol li a {
  color: #fff;
}

.top-nav-mega-full-width h1, .top-nav-mega-full-width h2, .top-nav-mega-full-width h3, .top-nav-mega-full-width h4, .top-nav-mega-full-width h5, .top-nav-mega-full-width h6 {
  color: #fff;
}

/* Items with images */
.item-with-images, .item-with-images-black {
  background: rgb(224,224,224);
  background: rgba(224,224,224,0.8);
  color: #333;
}

.item-with-images-black {
  background: rgb(0,0,0);
  background: rgba(0,0,0,0.8);
}

/* News Nav */
.mega-news-item {
  background: #3c4248;
  color: #fff;
}

.mega-news-item img {
  background: rgb(255,255,255);
  background: rgba(255,255,255,0.3);
}

.mega-news-item h3, .mega-news-item h3 a {
  color: #1b1b1b;
}

/* Top-nav News Nav title */
.mega-news-item h3 a {
  color: #e1e4e8;
}

/* Top-nav 1st level */
.top-nav > ul > li > a {
  background: #7d8c9d;
  background: rgba(125,140,157);
  color: #fff;
}

.top-nav > ul > li:hover > a {
  background: #39434d;
  background: rgba(57,67,77,1);
  color: #fff;
}

.fw-hovered {
  background: #39434d; /* fallback for older browsers */
  background: rgba(57,67,77,1);
  color: #fff;
}

/* Top-nav 2nd level */
.top-nav > ul > li > ul > li > a {
  background: #556270;
  background: rgba(85,98,112,0.8);
  border-top: 1px solid #7e8d9e;
  border-bottom: 1px solid #556270;
  color: #fff;
}

.top-nav > ul > li > ul > li > a:hover {
  background: #39434d;
  background: rgba(57,67,77,0.9);
  border-bottom: 1px solid #39434d;
  border-top: 1px solid #7e8d9e;
  color: #fff;
}

/* Top-nav 3rd level */
.top-nav > ul > li > ul > li > ul > li > a {
  background: #6d7e92;
  background: rgba(109,126,146,0.8);
  border-top: 1px solid #a3adba;
  border-bottom: 1px solid #51606f;
  color: #fff;
}

.top-nav > ul > li > ul > li > ul > li a:hover {
  background: #5a697a;
  background: rgba(90,105,122,0.8);
  border-top: 1px solid #a4aeb9;
  border-bottom: 1px solid #556270;
  color: #fff;
}

/* Top-nav 4th level */
.top-nav > ul > li > ul > li > ul > li > ul > li > a {
  background: #a6b0bd;
  background: rgba(166,176,189,0.9);
  border-top: 1px solid #d8dce2;
  border-bottom: 1px solid #8e9bac;
  color: #333;
}

.top-nav > ul > li > ul > li > ul > li > ul > li a:hover {
  background: #dbdee3;
  background: rgba(219,222,227,0.9);
  border-top: 1px solid #eef0f2;
  border-bottom: 1px solid #c5cad1;
  color: #000;
}

/* Top-nav input styles */
.top-nav input[type="text"], .top-nav textarea {
  background: #e1e4e8;
  color: #333;
}

.top-nav input[type="text"]:focus, .top-nav input:active, .top-nav textarea:focus {
  border: none;
  outline: none;
  border: 1px solid #000000;
}

/* Social media button styles */
.facebook, .twitter, .google-plus, .pinterest, .instagram, .linkedin, .yahoo, .youtube, .vimeo {
  background: rgb(142,155,170);
  background-repeat: no-repeat;
  background-position: 8px 0px;
  border-left: 1px solid #b1bbc5;
}

.facebook, .facebook:hover {
  background-image: url(../img/facebook.png);
}

.twitter, .twitter:hover {
  background-image: url(../img/twitter.png);
}

.google-plus, .google-plus:hover {
  background-image: url(../img/googleplus.png);
}

.pinterest, .pinterest:hover {
  background-image: url(../img/pinterest.png);
}

.linkedin, .linkedin:hover {
  background-image: url(../img/linkedin.png);
}

.yahoo, .yahoo:hover {
  background-image: url(../img/yahoo.png);
}

.youtube, .youtube:hover {
  background-image: url(../img/youtube.png);
}

.vimeo, .vimeo:hover {
  background-image: url(../img/vimeo.png);
}

/* <HR> horizontal line */
.top-nav-mega-content-seperator {
  border-top: 1px solid #65727c;
  border-bottom: 1px solid #3c4248;
}